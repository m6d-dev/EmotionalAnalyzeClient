{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Отладчик Python: Django",
      "type": "debugpy",
      "request": "launch",
      "args": [
        "runserver"
      ],
      "django": true,
      "autoStartBrowser": false,
      "program": "${workspaceFolder}/manage.py"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [
        "mkcldedcds"
      ],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/muhammad/projects/EmotionalAnalyze/client",
      "program": "/home/muhammad/projects/EmotionalAnalyze/client/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}